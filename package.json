{
  "name": "@cosmicjs/cli",
  "version": "1.0.0",
  "description": "AI-powered CLI for managing Cosmic CMS through natural language and direct commands",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "cosmic": "./bin/cosmic"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "start": "node dist/index.js",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write src/**/*.ts",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "cosmic",
    "cms",
    "cli",
    "headless-cms",
    "ai",
    "content-management"
  ],
  "author": "Cosmic <hello@cosmicjs.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cosmicjs/cli.git"
  },
  "bugs": {
    "url": "https://github.com/cosmicjs/cli/issues"
  },
  "homepage": "https://www.cosmicjs.com",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@cosmicjs/sdk": "^1.6.0",
    "axios": "^1.6.0",
    "chalk": "^5.3.0",
    "cli-table3": "^0.6.3",
    "commander": "^12.0.0",
    "conf": "^12.0.0",
    "enquirer": "^2.4.1",
    "eventsource": "^2.0.2",
    "marked": "^12.0.0",
    "marked-terminal": "^7.0.0",
    "open": "^10.0.0",
    "ora": "^8.0.0"
  },
  "devDependencies": {
    "@types/eventsource": "^1.1.15",
    "@types/node": "^20.0.0",
    "prettier": "^3.2.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0"
  }
}
